{
  "cli": {
    "version": ">=16.17.4",
    "appVersionSource": "remote"
  },
  "build": {
    "internal-apk": {
      "distribution": "internal",
      "developmentClient": false,
      "android": { "buildType": "apk" }
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": { "buildType": "apk" }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "android": { "buildType": "app-bundle", "developmentClient": false }
    }
  },
  "submit": {
    "production": { "android": {} }
  }
}
